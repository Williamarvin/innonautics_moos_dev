  button_seventeen  = SYNCH     # DEMUSTER_CONFIG_ALL = synch_toggle
  button_eighteen   = TYPE      # DEMUSTER_CONFIG     = type_toggle
  button_nineteen   = ALGORITHM # DEMUSTER_CONFIG     = algorithm_toggle
